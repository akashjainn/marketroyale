{
  "buildCommand": "pnpm -w run build:all",
  "outputDirectory": "dist",
  "functions": {
    "api/minimal.js": {
      "runtime": "nodejs18.x"
    },
    "api/debug.ts": {
      "runtime": "nodejs18.x"
    },
    "api/[...all].ts": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
